{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "webidl",
      "camelcase": "WebIDL",
      "title": "Webidl",
      "scope": "source.webidl",
      "file-types": [
        "webidl"
      ],
      "injection-regex": "^webidl$",
      "class-name": "TreeSitterWebidl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "tree-sitter grammar for the Web Interface Definition Language",
    "authors": [
      {
        "name": "Paul Adenot",
        "email": "padenot@mozilla.com",
        "url": "https://paul.cx/"
      }
    ],
    "links": {
      "repository": "https://github.com/padenot/tree-sitter-webidl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
